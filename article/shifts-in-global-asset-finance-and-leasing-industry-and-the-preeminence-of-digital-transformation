<script>

    $(document).ready(function(){
        
        $('#articleShareNew').click(function(){
            $('.articlesShareEl').toggleClass('active')
        })
 
        $('#copyArticleLink').click(function(){
            var copyText = document.getElementById("copyArticleLinkVal");
            copyText.select();
            document.execCommand("copy");
            //alert("Copied the text: " + copyText.value);
        })

        $('.btn.btn-tag').click(function(){
            if($(this).hasClass('active')){
                $('.btn.btn-tag').removeClass('active');
                $('.articleTagsList').removeClass('active');
                return false;
            }

            $('.btn.btn-tag').removeClass('active')
            $('.articleTagsList').removeClass('active')
            $(this).addClass('active')
            var href = $(this).attr('href');
            $(href).toggleClass('active')
            
            return false;
        })

        $(window).scroll(function(){
            var articleMainPos = parseInt($(".articleMain").offset().top - 120);
            var articleDetailPos = parseInt($(".articleDetail").offset().top);
            //console.log(articleMainPos+"----"+$(this).scrollTop)
            if($(this).scrollTop() > articleMainPos)
            {
                $("#articleDetailSticky").addClass('active');
                var articleDetailStickyHeight = $('#articleDetailSticky').outerHeight();
                var s = $(window).scrollTop() - ( articleDetailPos - articleDetailStickyHeight );
                d = $('.articleDetail').height();
                c = $(window).height();
                scrollPercent = (s / d) * 100;
                var position = parseInt(scrollPercent);
                $("#articleReadPercentage").css('width', position+'%');
            }
            else
            {
                $("#articleDetailSticky").removeClass('active')
                $("#articleReadPercentage").css('width', '0%');
            }
        })
    });
	
	
